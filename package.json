{
  "name": "ayanbhadury-grafana-fusioncharts-plugin",
  "version": "1.0.0",
  "description": "Panel used to visualize fusioncharts in grafana environment",
  "scripts": {
    "build": "grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "sign": "grafana-toolkit plugin:sign",
    "start": "yarn watch"
  },
  "author": "AyanBhadury",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/data": "latest",
    "@grafana/toolkit": "latest",
    "@grafana/ui": "latest",
    "emotion": "10.0.27"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "fusioncharts": "^3.18.0",
    "react-fusioncharts": "^4.0.0"
  }
}
